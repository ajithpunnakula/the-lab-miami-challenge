{
  "version": 2,
  "builds": [
    {
      "src": "calendar_agent/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "calendar_agent/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "calendar_agent/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "crons": [
    {
      "path": "/api/digest",
      "schedule": "0 9 * * 1"
    }
  ]
}